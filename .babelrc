{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": "last 2 versions",
          "node": "current"
        },
        "loose": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    ["@babel/plugin-transform-runtime", {
        "helpers": false,
        "regenerator": true
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "@babel/plugin-transform-react-constant-elements"
      ]
    },
    "test": {
      "plugins": [
        "@babel/plugin-transform-classes"
      ]
    }
  }
}
